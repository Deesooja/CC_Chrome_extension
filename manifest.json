{
    "manifest_version": 3,
    "name": "AppConnect",
    "version": "1.0.0",
    "permissions": [
      "activeTab",
      "scripting",
      "storage",
      "tabs"
    ],
    "host_permissions": ["https://*/*"],
    "icons": {
      "128": "icon.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "./background.mjs",
      "type": "module"
  }
  }
  